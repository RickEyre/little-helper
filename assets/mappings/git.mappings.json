{
  "command": "git",
  "alias": "g",
  "default": "help",
  "mappings": [{
    "command": "fetch",
    "alias": "f",
    "default": "upstream master",
    "formula": "%command% %1% %2% %options%"
  }, {
    "command": "rebase",
    "alias": "r",
    "default": "master",
    "formula": "%command% %1% %always% %options%",
    "options": {
      "h": "HEAD~%argument%",
      "t": "temporary"
    }
  }, {
    "command": "commit",
    "alias": "c",
    "default": "-a",
    "options": {
      "m": "--amend"
    }
  }, {
    "command": "clone",
    "alias": "cl"
  }, {
    "command": "push",
    "alias": "p",
    "formula": "%command% %1% %2% %options%",
    "options": {
      "o": "origin",
      "m": "master",
      "u": "upstream"
    }
  }, {
    "command": "pull",
    "alias": "pu",
    "formula": "%command% %1% %2% %options%",
    "options": {
      "o": "origin",
      "m": "master",
      "u": "upstream"
    }
  }, {
    "command": "diff",
    "alias": "d",
    "always": "--color",
    "options": {
      "h": "HEAD~%argument%"
    }
  }, {
    "command": "format-patch",
    "alias": "fp",
    "always": "-kp -U8",
    "default": "HEAD~1",
    "options": {
      "h": "HEAD~%argument%"
    }
  }, {
    "command": "reset",
    "alias": "rs",
    "always": "--hard",
    "options": {
      "h": "HEAD~%argument%"
    }
  }, {
    "command": "symbolic-ref",
    "alias": "bn",
    "default": "-q --short HEAD"
  }, {
    "command": "checkout",
    "alias": "co",
    "options": {
      "t": "-b temporary",
      "d": "-D temporary"
    }
  }, {
    "command": "status",
    "alias": "s"
  }, {
    "command": "log",
    "alias": "l"
  }, {
    "command": "merge",
    "alias": "m"
  }, {
    "command": "branch",
    "alias": "b"
  }, {
    "command": "add",
    "alias": "a"
  }]
}